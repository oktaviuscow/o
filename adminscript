    local function buystock(nameofstock)
        local args = {
            [1] = "General",
            [2] = "Merchant",
            [3] = "Buy",
            [4] = nameofstock,
            [5] = 1,
            [6] = "Trial Shard"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Bridge"):FireServer(unpack(args))
        
